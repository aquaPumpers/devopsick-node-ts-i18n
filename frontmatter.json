  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "GeneralFields",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "name": "fieldCollection",
          "type": "fieldCollection",
          "fieldGroup": "GeneralFields"
        }
      ]
    },
    {
      "id": "author",
      "labelField": "name",
      "fields": [
        {
          "title": "Author Name",
          "name": "name",
          "type": "string",
          "single": true
        },
        {
          "title": "Social link",
          "name": "social",
          "type": "string",
          "single": false
        }
      ]
    }
  ],
  "frontMatter.data.files": [
    {
      "id": "authors",
      "title": "Authors",
      "file": "[[workspace]]/data/authors.json",
      "schema": {
        "title": "Author",
        "type": "object",
        "required": [
          "name",
          "url"
        ],
        "properties": {
          "slug": {
            "title": "slug",
            "type": "string"
          },
          "name": {
            "title": "name",
            "type": "string"
          },
          "image": {},
          "bio": {
            "type": "string",
            "title": "bio",
            "multiline": true
          },
          "url": {
            "title": "url",
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.dashboard.openOnStart": false
}